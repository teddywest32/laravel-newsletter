{
    "name": "spatie/laravel-newsletter",
    "description": "Manage newsletters in Laravel 5",
    "keywords": [
        "laravel",
        "newsletter",
        "mailchimp"
    ],
    "homepage": "https://github.com/spatie/laravel-newsletter",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://murze.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.5|^7.0",
        "illuminate/support": "^5.1",
        "drewm/mailchimp-api": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.8",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Newsletter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Newsletter\\Test\\": "tests"
        }
    }
}
